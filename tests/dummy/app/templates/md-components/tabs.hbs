            {{/wb-md-code-highlight}}
        </div>
        <h3 class="guideline-block__chapter-title">Config Object</h3>
        {{#wb-md-code-highlight type="js" }}
            classicTabsConfig: {
                isDeletable: true,
                tabs: Ember.A([
                    Ember.Object.create({label: "Tab 1"}),
                    ...
                    Ember.Object.create({label:"Tab 4"}),
                ]) ,
                confirmationConfig: {
                    cancelButtonLabel: 'No',
                    confirmButonLabel: 'Yes',
                    title: 'Delete Custom Title',
                    message: '<p>Custom Information</p><p>Are you sure to perform the action?</p>'
                },
            };
        {{/wb-md-code-highlight}}
        <div class="guideline-block__chapter-inner">
            <div class="guideline-block__description">
                <h3 class="guideline-block__chapter-title">Preview</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-11"></div>
            <div class="col-md-1">{{wb-md-button label='+ ADD' size="small" click=(action "addTab") type='link'
                                                 color="blue"}}</div>
        </div>
        {{wb-md-button type="primary" label='Manage SCB Network Countries' color="blue" action="changeDeaultTab"}}
        {{wb-md-tabs selectedIndex=defaultTab onClick='clicked' config=classicTabsConfig onDelete=(action "deleteAction")}}
    </div>
