                &#123;{wb-md-tag config=toolTipConfig}}
            {{/wb-md-code-highlight}}
            <h3 class="guideline-block__chapter-title">Config Object</h3>
            {{#wb-md-code-highlight type="js"}}
            {
                label: 'Multiple',
                tooltip: {
                    //tooltip setting here
                    tags: [{
                            label: '2 Add',
                            circle: 'green'
                        },
                        {
                            label: '1 Delete',
                            circle: 'red'
                        }
                    ]
                    //end of tooltip setting
                }
            }
            {{/wb-md-code-highlight}}
        </div>
    </div>
