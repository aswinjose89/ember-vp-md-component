{{#wb-md-card}}
    <div class="guideline-block">
        {{#wb-md-content-toolbar pad="null"}}
            <div class="content-toolbar__row">
                <div class="content-toolbar__element">
                    <div class="content-toolbar__title content-toolbar__title_color_blue">Tags</div>
                </div>
                <div class="content-toolbar__element">
                    <div class="guideline-block__status guideline-block__status_stable">
                        DEV READY: stable
                    </div>
                </div>
            </div>
        {{/wb-md-content-toolbar}}
        <div class="guideline-block__example">
            {{wb-md-tag label="MULTIPLE"}}
        </div>
        <!--HTML Chapter-->
        <div class="guideline-block__chapter">
            <h3 class="guideline-block__chapter-title">HTML</h3>
            {{#wb-md-code-highlight}}
                <div class="global-tag">
                    <div class="global-tag__value">MULTIPLE</div>
                </div>
            {{/wb-md-code-highlight}}
        </div>
        <!--Ember Chapter-->
        <div class="guideline-block__chapter">
            <h3 class="guideline-block__chapter-title">Ember component</h3>
            {{#wb-md-code-highlight type="handlebars"}}
                &#123;{wb-md-tag label="MULTIPLE"}}
            {{/wb-md-code-highlight}}
            <div class="guideline-block__chapter-inner">
                <div class="guideline-block__description">
                    <h3 class="guideline-block__chapter-title">Settings</h3>
                    <ul>
                        <li><code><span>label</span></code> - Set tag text</li>
                        <li><code><span>color</span></code> - Set of tag background color.</li>
                    </ul>
                    <table>
                        <thead>
                        <tr>
                            <th>none</th>
                            <th>green</th>
                            <th>deep-orange</th>
                            <th>blue-gray</th>
                            <th>blue</th>
                            <th>orange</th>
                            <th>cyan</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td>{{wb-md-tag label="MULTIPLE"}}</td>
                            <td>{{wb-md-tag label="ADD" color="green"}}</td>
                            <td>{{wb-md-tag label="DELETE" color="deep-orange"}}</td>
                            <td>{{wb-md-tag label="REPLACE" color="blue-gray"}}</td>
                            <td>{{wb-md-tag label="MANDATED" color="blue"}}</td>
                            <td>{{wb-md-tag label="draft" color="orange"}}</td>
                            <td>{{wb-md-tag label="label" color="cyan"}}</td>
                        </tr>
                        </tbody>
                    </table>
                    <ul>
                        <li><code><span>circle</span></code> - Set of tag circle color.
                            <div class="guideline-block__example">
                                <table>
                                    <thead>
                                    <tr>
                                        <th>gray</th>
                                        <th>green</th>
                                        <th>deep-orange</th>
                                        <th>blue-gray</th>
                                        <th>blue</th>
                                        <th>orange</th>
                                        <th>red</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    <tr>
                                        <td>{{wb-md-tag label="Draft" circle="gray"}}</td>
                                        <td>{{wb-md-tag label="ADD" circle="green"}}</td>
                                        <td>{{wb-md-tag label="DELETE" circle="deep-orange"}}</td>
                                        <td>{{wb-md-tag label="REPLACE" circle="blue-gray"}}</td>
                                        <td>{{wb-md-tag label="MANDATED" circle="blue"}}</td>
                                        <td>{{wb-md-tag label="draft" circle="orange"}}</td>
                                        <td>{{wb-md-tag label="High" circle="red"}}</td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
{{/wb-md-card}}
