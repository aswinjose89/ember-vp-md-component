                                            <td>{{wb-md-entity-icon value="D" color="blue-800"}}</td>
                                            <td>{{wb-md-entity-icon value="D" color="green-500"}}</td>
                                            <td>{{wb-md-entity-icon value="D" color="indigo-400"}}</td>
