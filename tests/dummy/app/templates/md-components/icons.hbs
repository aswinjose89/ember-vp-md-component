{{#wb-md-card}}
    <div class="guideline-block">
        {{#wb-md-content-toolbar pad="null" }}
            <div class="content-toolbar__row">
                <div class="content-toolbar__element">
                    <div class="content-toolbar__title content-toolbar__title_color_blue">Icon</div>
                </div>
                <div class="content-toolbar__element">
                    <div class="guideline-block__status guideline-block__status_stable">
                        DEV READY: stable
                    </div>
                </div>
            </div>
        {{/wb-md-content-toolbar}}
        <!--Ember Chapter-->
        <div class="guideline-block__chapter">
            <h3 class="guideline-block__chapter-title">Ember component</h3>
            {{#wb-md-code-highlight type="handlebars" }}
                &#123;{wb-md-icon iconName=iconName}}
            {{/wb-md-code-highlight}}
            <div class="guideline-block__chapter-inner">
                <div class="guideline-block__description">
                    <h3 class="guideline-block__chapter-title">Settings</h3>
                    <ul>
                        <li>
                            <code><span>color</span></code> - Change icon color.
                            <div class="guideline-block__example">
                                <table>
                                    <thead>
                                        <tr>
                                            <th>black</th>
                                            <th>blue</th>
                                            <th>blue-500</th>
                                            <th>blue-gray-800</th>
                                            <th>gray-light</th>
                                            <th>green-500</th>
                                            <th>muleFawn</th>
                                            <th>orange-400</th>
                                            <th>orange-500</th>
                                            <th>red-500</th>
                                            <th>white</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>{{wb-md-icon iconName="alarm" color="black"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="blue"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="blue-500"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="blue-gray-800"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="gray-light"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="green-500"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="muleFawn"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="orange-400"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="orange-500"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="red-500"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="white"}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </li>
                        <li>
                            <code><span>size</span></code> - Icon Size
                            <div class="guideline-block__example">
                                <table>
                                    <thead>
                                        <tr>
                                            <th>default</th>
                                            <th>medium</th>
                                            <th>small</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>{{wb-md-icon iconName="alarm" color="black"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="black" size="medium"}}</td>
                                            <td>{{wb-md-icon iconName="alarm" color="black" size="small"}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </li>
                        <li>
                            <code><span>active</span></code> - false by default.
                            <div class="guideline-block__example">
                                {{wb-md-icon iconName="alarm" color="black" active=true}}
                            </div>
                        </li>
                        <li>
                            <code><span>isDisabled</span></code> - false by default.
                            <div class="guideline-block__example">
                                {{wb-md-icon iconName="alarm" color="black" isDisabled=true}}
                            </div>
                        </li>
                        <li>
                            <code><span>isReadOnly</span></code> - false by default.
                            <div class="guideline-block__example">
                                {{wb-md-icon iconName="alarm" color="black" isReadOnly=true}}
                            </div>
                        </li>
                        <li>
                            <code><span>helpText</span></code> - tooltip text.
                            <div class="guideline-block__example">
                                {{wb-md-icon iconName="alarm" color="black" helpText="short text here"}}
                            </div>
                        </li>
                        <li>
                            <code><span>Actions</span></code>
                            <ul>
                                <li><code><span>onClickAction</span></code> - invoke on click action</li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
            {{icons-list iconsList=model}}
        </div>
    </div>
{{/wb-md-card}}
