                                <table>
                                    <thead>
                                        <tr>
                                            <th>none</th>
                                            <th>medium</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td>{{wb-md-entity-icon value="D"}}</td>
                                            <td>{{wb-md-entity-icon value="112" size="medium"}}{{wb-md-entity-icon value="D" size="medium"}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
